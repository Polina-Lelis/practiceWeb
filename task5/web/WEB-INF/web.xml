<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>Servlet</servlet-name>
        <servlet-class>main.java.bsu.com.Servlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>Servlet</servlet-name>
        <url-pattern>/get</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Servlet_Post</servlet-name>
        <servlet-class>main.java.bsu.com.Servlet_Post</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Servlet_Post</servlet-name>
        <url-pattern>/check</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>status</servlet-name>
        <jsp-file>/status.jsp</jsp-file>
    </servlet>

    <servlet-mapping>
        <servlet-name>status</servlet-name>
        <url-pattern>/status</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>page</servlet-name>
        <jsp-file>/page.jsp</jsp-file>
    </servlet>

    <servlet-mapping>
        <servlet-name>page</servlet-name>
        <url-pattern>/page</url-pattern>
    </servlet-mapping>



